<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="orm.Establecimiento" table="establecimiento" lazy="false">
		<id name="id" column="id" type="integer" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
		<property name="nombre" column="nombre" type="string" length="255" not-null="true" lazy="false">
			<meta attribute="field-description">
				nombre del establecimiento
			</meta>
		</property>
		<set name="ORM_Medico" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="idEstablecimiento" not-null="true"/>
			<one-to-many class="orm.Medico"/>
		</set>
		<set name="ORM_HoraMedica" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="idEstablecimiento" not-null="true"/>
			<one-to-many class="orm.HoraMedica"/>
		</set>
		<set name="ORM_Box" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="idEstablecimiento" not-null="true"/>
			<one-to-many class="orm.Box"/>
		</set>
	</class>
</hibernate-mapping>
